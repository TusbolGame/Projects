<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle">
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#f1e507" />
            <gradient android:angle="-90" android:startColor="#eaf206" android:endColor="#dff704"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#eef486" />
            <solid android:color="#f5f765"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#f79f06" />
            <gradient android:angle="-90" android:startColor="#f48d08" android:endColor="#b45c03" />
        </shape>
    </item>
</selector>
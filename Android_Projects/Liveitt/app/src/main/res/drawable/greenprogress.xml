            <corners n1:radius="5.0dip" />
            <gradient n1:startColor="@color/dark_gray" n1:endColor="@color/dark_gray" n1:angle="270.0" n1:centerY="0.75" n1:centerColor="@color/dark_gray" />
        </shape>

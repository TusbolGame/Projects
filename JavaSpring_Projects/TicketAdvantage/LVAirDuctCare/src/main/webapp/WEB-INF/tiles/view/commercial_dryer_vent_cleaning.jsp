<%@ taglib prefix="spring" uri="http://www.springframework.org/tags"%>
<section class="banner_area aos-init" data-aos="fade-up"
	data-aos-duration="600">
	<div class="row">
		<div class="col-md-4 col-sm-4 col-xs-12 banner_bg dark_blue text-left">
			<div class="banner_inner">
				<h1><spring:message code="commercial" text="default" /> <br><spring:message code="dryerventcleaningbasic" text="default" /></h1>
			</div>
		</div>
		<div class="col-md-8 col-sm-8 col-xs-12 padding_none text-right">
			<img src="images/commercial_dryer_vent_cleaning_banner.jpg" alt="">
		</div>
	</div>
</section>
<section class="service_area">
	<div class="container">
		<div class="row">
			<div class="col-md-12 col-sm-12 col-xs-12 about_content">
				<div class="col-xs-12 col-md-6 col-sm-12 aos-init" data-aos="fade-right" data-aos-duration="600">
					<h3><span><spring:message code="welcometo" text="default" /></span> <spring:message code="commericaldryerventcleaning" text="default" /></h3>
					<p><spring:message code="integral" text="default" /></p>
				</div>
				<div class="col-xs-12 col-md-6 col-sm-12 aos-init text-center" data-aos="fade-left" data-aos-duration="600">
					<img src="images/las-vegas-skyline.jpg" alt="">
				</div>
			</div>
		</div>
		<h2><spring:message code="warningsymptoms" text="default" /></h2>
		<div class="row">
			<div class="col-md-12 col-sm-12 col-xs-12 about_content">
				<div class="col-xs-12 col-md-6 col-sm-12 aos-init" data-aos="fade-right" data-aos-duration="600">
					<ul>
						<li><i class="fa fa-hand-o-right"></i> <spring:message code="excess" text="default" /></li>
						<li><i class="fa fa-hand-o-right"></i> <spring:message code="hottouch" text="default" /></li>
						<li><i class="fa fa-hand-o-right"></i> <spring:message code="lintvisible" text="default" /></li>
					</ul>
				</div>
				<div class="col-xs-12 col-md-6 col-sm-12 aos-init" data-aos="fade-left" data-aos-duration="600">
					<ul>
						<li><i class="fa fa-hand-o-right"></i> <spring:message code="shutsoff" text="default" /></li>
						<li><i class="fa fa-hand-o-right"></i> <spring:message code="increaseddrying" text="default" /></li>
						<li><i class="fa fa-hand-o-right"></i> <spring:message code="clotheshot" text="default" /></li>
					</ul>
				</div>
			</div>
		</div>
	</div>
</section>
<section class="quote_area dark_blue_bg aos-init" data-aos="fade-up" data-aos-duration="600">
	<h2><spring:message code="scheduleestimate" text="default"/></h2>
	<p><spring:message code="flatratepricing" text="default"/></p>
	<button type="button" class="" data-toggle="modal" data-target="#modalCenter"><spring:message code="getafreequote" text="default"/></button>
</section>
<section class="service_area">
	<div class="container">
		<div class="row">
			<div class="col-md-12 col-sm-12 col-xs-12 about_content">
				<div class="col-xs-12 col-md-6 col-sm-12 aos-init text-center" data-aos="fade-left" data-aos-duration="600">
					<img src="images/negativeair.png" alt="">
				</div>
				<div class="col-xs-12 col-md-6 col-sm-12 aos-init" data-aos="fade-right" data-aos-duration="600">
					<h3><spring:message code="shouldknow" text="default" /></h3>
					<p><spring:message code="requiredmaintenance" text="default" /></p>
				</div>
			</div>
		</div>
	</div>
</section>
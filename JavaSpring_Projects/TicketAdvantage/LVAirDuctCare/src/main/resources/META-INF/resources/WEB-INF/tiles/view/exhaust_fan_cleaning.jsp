<%@ taglib prefix="spring" uri="http://www.springframework.org/tags" %>
<section class="banner_area aos-init" data-aos="fade-up"
	data-aos-duration="600">
	<div class="row">
		<div class="col-md-4 col-sm-4 col-xs-12 banner_bg blue text-right">
			<div class="banner_inner">
				<h1><spring:message code="exhaustfancleaning" text="default"/></h1>
			</div>
		</div>
		<div class="col-md-8 col-sm-8 col-xs-12 padding_none text-right">
			<img src="images/exhaust_fan_cleaning_home.jpg" alt="">
		</div>
	</div>
</section>
<section class="service_area">
	<div class="container">
		<div class="row">
			<div class="col-md-12 col-sm-12 col-xs-12 about_content">
				<div class="col-md-6 col-sm-12 col-xs-12 aos-init" data-aos="fade-right" data-aos-duration="600">
					<h3>&nbsp;<spring:message code="lvexhaust" text="default"/></h3>
					<p><spring:message code="veryimportant" text="default"/></p>
				</div>
				<div class="col-md-6 col-sm-12 col-xs-12 aos-init text-center" data-aos="fade-left" data-aos-duration="600">
					<img src="images/vent-cleaning.jpg" alt="">
				</div>
			</div>
		</div>
		<div class="row">
			<div class="col-md-12 col-sm-12 col-xs-12 about_content">
				<div class="col-xs-12 col-md-6 col-sm-12 aos-init text-center" data-aos="fade-left" data-aos-duration="600">
					<img src="images/exhaust_fan_cleaning.jpg" alt="">
				</div>
				<div class="col-xs-12 col-md-6 col-sm-12 aos-init" data-aos="fade-right" data-aos-duration="600">
					<h3><spring:message code="exhaustfancleaning" text="default"/></h3>
					<p><spring:message code="moisture" text="default"/></p>
				</div>
			</div>
		</div>
	</div>
</section>
<section class="quote_area blue_bg aos-init" data-aos="fade-up" data-aos-duration="600">
	<h2><spring:message code="scheduleestimate" text="default"/></h2>
	<p><spring:message code="flatratepricing" text="default"/></p>
	<button type="button" class="" data-toggle="modal" data-target="#modalCenter"><spring:message code="getafreequote" text="default"/></button>
</section>
<section class="service_area">
	<div class="container">
		<div class="row">
			<div class="col-md-12 col-sm-12 col-xs-12 about_content">
				<div class="col-xs-12 col-md-6 col-sm-12 aos-init"
					data-aos="fade-right" data-aos-duration="600">
					<h3>
						<spring:message code="scheduleappointment" text="default"/> <strong><spring:message code="callus" text="default"/></strong>
					</h3>
					<ul>
						<li><i class="fa fa-hand-o-right"></i> <spring:message code="freeservicecall" text="default"/></li>
						<li><i class="fa fa-hand-o-right"></i> <spring:message code="guaranteedsatisfaction" text="default"/></li>
						<li><i class="fa fa-hand-o-right"></i> <spring:message code="affordablepricing" text="default"/></li>
						<li><i class="fa fa-hand-o-right"></i> <spring:message code="licensedinsured" text="default"/></li>
					</ul>
				</div>
			</div>
		</div>
	</div>
</section>
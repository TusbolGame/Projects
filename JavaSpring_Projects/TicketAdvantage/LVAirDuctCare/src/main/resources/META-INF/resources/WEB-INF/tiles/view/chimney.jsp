<%@ taglib prefix="spring" uri="http://www.springframework.org/tags" %>
<section class="banner_area aos-init" data-aos="fade-up"
	data-aos-duration="600">
	<div class="row">
		<div class="col-md-4 col-sm-4 col-xs-12 banner_bg cyan text-left">
			<div class="banner_inner">
				<h1><spring:message code="residential" text="default"/> <br/><spring:message code="chimney" text="default"/> <br/><spring:message code="sweep" text="default"/></h1>
			</div>
		</div>
		<div class="col-md-8 col-sm-8 col-xs-12 padding_none text-right">
			<img src="images/clean-chimney_300_200.jpg" alt="">
		</div>
	</div>
</section>
<section class="service_area">
	<div class="container">
		<div class="row">
			<div class="col-md-12 col-sm-12 col-xs-12 about_content">
				<div class="col-xs-12 col-md-6 col-sm-12 aos-init" data-aos="fade-right" data-aos-duration="600">
					<h3><span><spring:message code="welcometo" text="default"/></span> <spring:message code="chimneycleaningservice" text="default"/></h3>
					<p><spring:message code="chimneyintro" text="default"/></p>
				</div>
				<div class="col-xs-12 col-md-6 col-sm-12 aos-init text-center" data-aos="fade-left" data-aos-duration="600">
					<img src="images/chimneydirty.jpg" alt="">
				</div>
			</div>
		</div>
		<div class="row">
			<div class="col-md-12 col-sm-12 col-xs-12 about_content">
				<div class="col-xs-12 col-md-6 col-sm-12 aos-init text-center" data-aos="fade-left" data-aos-duration="600">
					<img src="images/chimneyclean.jpg" alt="">
				</div>
				<div class="col-xs-12 col-md-6 col-sm-12 aos-init" data-aos="fade-right" data-aos-duration="600">
					<p><spring:message code="chimneyfire" text="default"/></p>
				</div>
			</div>
		</div>
		<div class="row">
			<div class="col-md-12 col-sm-12 col-xs-12 about_content">
				<div class="col-xs-12 col-md-6 col-sm-12 aos-init" data-aos="fade-right" data-aos-duration="600">
					<p><spring:message code="chimneyregular" text="default"/></p>
				</div>
				<div class="col-xs-12 col-md-6 col-sm-12 aos-init text-center" data-aos="fade-left" data-aos-duration="600">
					<img src="images/chimney-sweep.jpg" alt="">
				</div>
			</div>
		</div>
	</div>
</section>
<section class="quote_area cyan_bg aos-init" data-aos="fade-up"data-aos-duration="600">
	<h2><spring:message code="scheduleestimate" text="default"/></h2>
	<p><spring:message code="flatratepricing" text="default"/></p>
	<button type="button" class="" data-toggle="modal" data-target="#modalCenter"><spring:message code="getafreequote" text="default"/></button>
</section>
<section class="service_area">
	<div class="container">
		<div class="row">
			<div class="col-md-12 col-sm-12 col-xs-12 about_content">
				<div class="col-xs-12 col-md-6 col-sm-12 aos-init" data-aos="fade-right" data-aos-duration="600">
					<h3>
						<spring:message code="chimneycleaningbasic" text="default"/> <strong><spring:message code="thebenefits" text="default"/></strong>
					</h3>
